[application]

config/name="HTML5 en Acción"
config/description="El videojuego HTML5 es una emocionante aventura que te sumerge en el mundo del desarrollo web y la tecnología. En este juego, asumes el papel de Tim Berners-Lee, un brillante pionero en la creación de la World Wide Web. Tu misión es recopilar información valiosa relacionada con HTML5 y otros estándares web para avanzar en tu carrera y superar desafíos."
run/main_scene="res://Escenas/Intro.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://Scripts/Globales/global_contador.gd"

[display]

window/size/width=1152
window/size/height=648
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/dialogic/plugin.cfg" )

